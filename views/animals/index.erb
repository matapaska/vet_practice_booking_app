<a class="link" href="/index/animals/new">Register animal</a>
<h3>Animals:</h3>
<div class="indexData">


  <% @animals.each do |animal| %>

    <h4><a href="/index/animals/<%= animal.id%>"><span id = "animal_name"><%= animal.name %></span></a></h4>
    <p><span>Type: </span><%= animal.type %></p>
    <p><span>Vet:</span> <span id = "name"><%= animal.vets.name %></span></p>
    <p><span>Owner:</span> <a href="/index/owners/<%= animal.owners.id%>"><span id = "name"><%= animal.owners.name %></span></a></p>
<br>


  <% end %>

  </div>
